<?xml version="1.0"?>
<jxb:bindings version="1.0" 
	xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
	xmlns:inheritance="http://jaxb2-commons.dev.java.net/basic/inheritance"
	xmlns:abstract="http://www.example.com/jaxb/abstract-modifier/1-0"
	jxb:extensionBindingPrefixes="xjc">

	<jxb:bindings schemaLocation="elza-schema-v2.xsd">
		<jxb:bindings node="//xs:complexType[@name='DescriptionItem']">
			<inheritance:implements>cz.tacr.elza.deimport.sections.items.ImprotableItem</inheritance:implements>
			<jxb:class name="AbstractDescriptionItem"/>
		</jxb:bindings>
		<jxb:bindings node="//xs:complexType[@name='DescriptionItemPartyRef']">
			<inheritance:extends>cz.tacr.elza.deimport.sections.items.DescriptionItem</inheritance:extends>
			<jxb:class implClass="cz.tacr.elza.deimport.sections.items.DescriptionItemPartyRefImpl"/>
		</jxb:bindings>
		<jxb:bindings node="//xs:complexType[@name='DescriptionItemAPRef']">
			<inheritance:extends>cz.tacr.elza.deimport.sections.items.DescriptionItem</inheritance:extends>
			<jxb:class implClass="cz.tacr.elza.deimport.sections.items.DescriptionItemAPRefImpl"/>
		</jxb:bindings>
		<jxb:bindings node="//xs:complexType[@name='DescriptionItemInteger']">
			<inheritance:extends>cz.tacr.elza.deimport.sections.items.DescriptionItem</inheritance:extends>
			<jxb:class implClass="cz.tacr.elza.deimport.sections.items.DescriptionItemIntegerImpl"/>
		</jxb:bindings>
		<jxb:bindings node="//xs:complexType[@name='DescriptionItemUnitDate']">
			<inheritance:extends>cz.tacr.elza.deimport.sections.items.DescriptionItem</inheritance:extends>
			<jxb:class implClass="cz.tacr.elza.deimport.sections.items.DescriptionItemUnitDateImpl"/>
		</jxb:bindings>
		<jxb:bindings node="//xs:complexType[@name='DescriptionItemEnum']">
			<inheritance:extends>cz.tacr.elza.deimport.sections.items.DescriptionItem</inheritance:extends>
			<jxb:class implClass="cz.tacr.elza.deimport.sections.items.DescriptionItemEnumImpl"/>
		</jxb:bindings>
		<jxb:bindings node="//xs:complexType[@name='DescriptionItemPacketRef']">
			<inheritance:extends>cz.tacr.elza.deimport.sections.items.DescriptionItem</inheritance:extends>
			<jxb:class implClass="cz.tacr.elza.deimport.sections.items.DescriptionItemPacketRefImpl"/>
		</jxb:bindings> 
		<jxb:bindings node="//xs:complexType[@name='DescriptionItemDecimal']">
			<inheritance:extends>cz.tacr.elza.deimport.sections.items.DescriptionItem</inheritance:extends>
			<jxb:class implClass="cz.tacr.elza.deimport.sections.items.DescriptionItemDecimalImpl"/>
		</jxb:bindings>
		<jxb:bindings node="//xs:complexType[@name='DescriptionItemString']">
			<inheritance:extends>cz.tacr.elza.deimport.sections.items.DescriptionItem</inheritance:extends>
			<jxb:class implClass="cz.tacr.elza.deimport.sections.items.DescriptionItemStringImpl"/>
		</jxb:bindings>
	</jxb:bindings>

</jxb:bindings>