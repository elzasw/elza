<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

        <!--Verze 1.19 - entita par_abstract_party_name -->
        <changeSet id="19" author="kuzel" failOnError="false" runOnChange="true">

            <createTable tableName="par_abstract_party_name">
                <column name="abstract_party_name_id" type="int">
                    <constraints primaryKey="true" nullable="false"/>
                </column>
                <column name="abstract_party_id" type="int">
                    <constraints nullable="false"/>
                </column>
                <column name="name" type="text">
                    <constraints nullable="false"/>
                </column>
            </createTable>

        </changeSet>
</databaseChangeLog>










