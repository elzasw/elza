<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="22" author="slapa">

        <addColumn tableName="arr_data_unitdate">
            <column name="normalized_from" type="bigint" defaultValue="0">
                <constraints nullable="false"/>
            </column>
            <column name="normalized_to" type="bigint" defaultValue="0">
                <constraints nullable="false"/>
            </column>
        </addColumn>

        <dropDefaultValue columnDataType="bigint"
                          columnName="normalized_from"
                          tableName="arr_data_unitdate"/>

        <dropDefaultValue columnDataType="bigint"
                          columnName="normalized_to"
                          tableName="arr_data_unitdate"/>

    </changeSet>

</databaseChangeLog>