<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="60" author="slapa">
        <addColumn tableName="arr_change">
            <column name="type" type="char(25)" />
            <column name="primary_level_id" type="int" />
        </addColumn>

        <addForeignKeyConstraint constraintName="fk_change_primary_level"
                                 baseTableName="arr_change"
                                 baseColumnNames="primary_level_id"
                                 referencedTableName="arr_level"
                                 referencedColumnNames="level_id"
                                 onDelete="NO ACTION"/>
    </changeSet>

</databaseChangeLog>