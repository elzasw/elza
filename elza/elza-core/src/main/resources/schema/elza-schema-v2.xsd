<?xml version="1.0" encoding="utf-8"?>
<xs:schema targetNamespace="http://elza.tacr.cz/schema/v2" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:e="http://elza.tacr.cz/schema/v2">
	<xs:element name="edx" type="e:ElzaDataExchange">
		<xs:annotation>
			<xs:documentation>Root element for elza data exchange format</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:simpleType name="AccessPointType">
		<xs:annotation>
			<xs:documentation>Registry item type. Value have to be one of items stored in the table reg_registry_type.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="BinData">
		<xs:annotation>
			<xs:documentation>Encoded binary data</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:base64Binary"/>
	</xs:simpleType>
	<xs:simpleType name="GenericString">
		<xs:annotation>
			<xs:documentation>Generic string value. It may contain EOL and other formatting characters.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="Id">
		<xs:annotation>
			<xs:documentation>Id of the item. This id is valid and usable only in the context of given elza data exchange file</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="String250">
		<xs:annotation>
			<xs:documentation>String with max length 250 characters</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="Uuid">
		<xs:annotation>
			<xs:documentation>UUID</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:complexType name="AccessPoint">
		<xs:annotation>
			<xs:documentation>Access point with all attributes</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="chr" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Characteristics of record</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="nms" type="e:AccessPointNames" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Names of accesspoint

At least one name have to be defined.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ape" type="e:AccessPointEntry" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Entry infor for access point</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="frgs" type="e:Fragments" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AccessPointEntry">
		<xs:annotation>
			<xs:documentation>Registry item / one record</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="eid" type="e:ExternalId" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="id" use="required" type="e:Id"/>
		<xs:attribute name="t" use="required" type="e:AccessPointType">
			<xs:annotation>
				<xs:documentation>Type of access point</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="uuid" use="optional" type="e:Uuid">
			<xs:annotation>
				<xs:documentation>UUID of one registry record</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="AccessPointName">
		<xs:annotation>
			<xs:documentation>Variant name of record</xs:documentation>
		</xs:annotation>
		<xs:sequence/>
		<xs:attribute name="l" use="optional" type="xs:string">
			<xs:annotation>
				<xs:documentation>Language of name (ISO code, 3letters)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="n" use="required" type="xs:string">
			<xs:annotation>
				<xs:documentation>Name of access point</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="cpl" use="optional" type="xs:string">
			<xs:annotation>
				<xs:documentation>Complement</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="AccessPointNames">
		<xs:annotation>
			<xs:documentation>List of variant record names</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="nm" type="e:AccessPointName" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Names of access point. First name is prefered one.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AccessPointRefs">
		<xs:annotation>
			<xs:documentation>List of references to access points</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="apid" type="e:Id" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Access point ID</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AccessPoints">
		<xs:annotation>
			<xs:documentation>Registry record list</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ap" type="e:AccessPoint" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DescriptionItem" abstract="true">
		<xs:annotation>
			<xs:documentation>Abstract description item</xs:documentation>
		</xs:annotation>
		<xs:sequence/>
		<xs:attribute name="s" use="optional" type="xs:string">
			<xs:annotation>
				<xs:documentation>Specification of description item. Specification is subtype of the type.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="t" use="required" type="xs:string">
			<xs:annotation>
				<xs:documentation>Type of description item</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="DescriptionItemAPRef">
		<xs:annotation>
			<xs:documentation>Access point entry reference</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="e:DescriptionItem">
				<xs:sequence/>
				<xs:attribute name="apid" use="optional" type="e:Id">
					<xs:annotation>
						<xs:documentation>Acess point entry ID</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="DescriptionItemBit">
		<xs:annotation>
			<xs:documentation>Boolean value</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="e:DescriptionItem">
				<xs:sequence/>
				<xs:attribute name="v" use="required" type="xs:boolean">
					<xs:annotation>
						<xs:documentation>Value of the boolean item</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="DescriptionItemDecimal">
		<xs:annotation>
			<xs:documentation>Description Item for single decimal value</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="e:DescriptionItem">
				<xs:sequence/>
				<xs:attribute name="v" use="required" type="xs:decimal">
					<xs:annotation>
						<xs:documentation>Value
Type: decimal</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="DescriptionItemEnum">
		<xs:annotation>
			<xs:documentation>Description item for enumeration</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="e:DescriptionItem">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="DescriptionItemFileRef">
		<xs:annotation>
			<xs:documentation>Reference to the file</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="e:DescriptionItem">
				<xs:sequence/>
				<xs:attribute name="fid" use="optional" type="e:Id">
					<xs:annotation>
						<xs:documentation>Referenced file ID</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="DescriptionItemInteger">
		<xs:annotation>
			<xs:documentation>Description Item with single integer value</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="e:DescriptionItem">
				<xs:sequence/>
				<xs:attribute name="v" use="required" type="xs:integer">
					<xs:annotation>
						<xs:documentation>Value
Type: Integer</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="DescriptionItemPartyRef">
		<xs:annotation>
			<xs:documentation>Party reference</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="e:DescriptionItem">
				<xs:sequence/>
				<xs:attribute name="paid" use="optional" type="e:Id">
					<xs:annotation>
						<xs:documentation>Party ID</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="DescriptionItemString">
		<xs:annotation>
			<xs:documentation>Description item for single String value. This is most generic description item. It can be used to store values of other types.

Item is used to represent at least:
Coordinates serialized in string
Simple string value
Text  / LongText with unlimited length
Formatted text
Item ID</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="e:DescriptionItem">
				<xs:sequence>
					<xs:element name="v" type="e:GenericString" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>String value. String may contain EOL and other formatting characters.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="DescriptionItemStructObjectRef">
		<xs:annotation>
			<xs:documentation>Description item to reference structured object</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="e:DescriptionItem">
				<xs:sequence/>
				<xs:attribute name="soid" use="required" type="e:Id">
					<xs:annotation>
						<xs:documentation>ID of the structured object, this ID is valid only in XML</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="DescriptionItemUndefined">
		<xs:annotation>
			<xs:documentation>Value is undefined</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="e:DescriptionItem">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="DescriptionItemUnitDate">
		<xs:annotation>
			<xs:documentation>Date value.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="e:DescriptionItem">
				<xs:sequence>
					<xs:element name="d" type="e:TimeInterval" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Link to complex date. Complex date have to have valid data in f, to and fmt.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="DescriptionItemUriRef">
		<xs:annotation>
			<xs:documentation>Link to other item</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="e:DescriptionItem">
				<xs:sequence/>
				<xs:attribute name="lbl" use="optional" type="xs:string">
					<xs:annotation>
						<xs:documentation>Link label</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="schm" use="required" type="xs:string">
					<xs:annotation>
						<xs:documentation>Schema, typical values are: http, https, elza-node</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="uri" use="required" type="xs:string">
					<xs:annotation>
						<xs:documentation>URI of linked item</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ElzaDataExchange">
		<xs:annotation>
			<xs:documentation>Elza data</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="aps" type="e:AccessPoints" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>List of access points</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pars" type="e:Parties" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>List of parties.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="inss" type="e:Institutions" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Institutions</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="rels" type="e:Relations" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Party relations</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fs" type="e:Sections" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>List of funds</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Event">
		<xs:annotation>
			<xs:documentation>event</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="e:Party">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ExternalId">
		<xs:annotation>
			<xs:documentation>ID defined by external system</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="t" type="e:Id" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>External system ID code / type</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="v" type="e:Id" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>ID in external system</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Family">
		<xs:annotation>
			<xs:documentation>Family/dynasty description</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="e:Party">
				<xs:sequence>
					<xs:element name="gen" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Genealogy</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="File">
		<xs:annotation>
			<xs:documentation>Single file.
Each file has name and its content - binary data.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="d" type="e:BinData" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
		<xs:attribute name="fn" use="optional" type="e:String250">
			<xs:annotation>
				<xs:documentation>File name, original name of file</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="id" use="optional" type="e:Id">
			<xs:annotation>
				<xs:documentation>ID of attachment</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="mt" use="optional" type="e:String250">
			<xs:annotation>
				<xs:documentation>mime-type</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="n" use="required" type="e:String250">
			<xs:annotation>
				<xs:documentation>Name of file</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="Files">
		<xs:annotation>
			<xs:documentation>Collection of files</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="f" type="e:File" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Fragment">
		<xs:sequence>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Description item</xs:documentation>
				</xs:annotation>
				<xs:element name="dd" type="e:DescriptionItemDecimal" minOccurs="1" maxOccurs="1"/>
				<xs:element name="do" type="e:DescriptionItemStructObjectRef" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Description item for packet</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="dp" type="e:DescriptionItemPartyRef" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Party reference</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="dt" type="e:DescriptionItemUnitDate" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Description item date</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ds" type="e:DescriptionItemString" minOccurs="1" maxOccurs="1"/>
				<xs:element name="de" type="e:DescriptionItemEnum" minOccurs="1" maxOccurs="1"/>
				<xs:element name="di" type="e:DescriptionItemInteger" minOccurs="1" maxOccurs="1"/>
				<xs:element name="du" type="e:DescriptionItemUndefined" minOccurs="1" maxOccurs="1"/>
				<xs:element name="da" type="e:DescriptionItemAPRef" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Access point entry reference</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="df" type="e:DescriptionItemFileRef" minOccurs="1" maxOccurs="1"/>
				<xs:element name="db" type="e:DescriptionItemBit" minOccurs="1" maxOccurs="1"/>
				<xs:element name="dl" type="e:DescriptionItemUriRef" minOccurs="1" maxOccurs="1"/>
			</xs:choice>
		</xs:sequence>
		<xs:attribute name="fid" use="optional" type="e:Id">
			<xs:annotation>
				<xs:documentation>Fragment ID, valid only in XML, allows to reference such fragment</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="pid" use="optional" type="e:Id">
			<xs:annotation>
				<xs:documentation>Parent fragment</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="t" use="required" type="xs:string">
			<xs:annotation>
				<xs:documentation>Type of fragment</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="Fragments">
		<xs:annotation>
			<xs:documentation>Collection of fragments</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="frg" type="e:Fragment" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FundInfo">
		<xs:annotation>
			<xs:documentation>Basic fund information </xs:documentation>
		</xs:annotation>
		<xs:sequence/>
		<xs:attribute name="c" use="optional" type="xs:string">
			<xs:annotation>
				<xs:documentation>Internal code of fund (optional)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ic" use="required" type="xs:string">
			<xs:annotation>
				<xs:documentation>Institution code</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="n" use="required" type="xs:string">
			<xs:annotation>
				<xs:documentation>Fund name</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="tr" use="optional" type="xs:string">
			<xs:annotation>
				<xs:documentation>Time range of stored items</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="Institution">
		<xs:annotation>
			<xs:documentation>Single institution</xs:documentation>
		</xs:annotation>
		<xs:sequence/>
		<xs:attribute name="c" use="optional" type="xs:string">
			<xs:annotation>
				<xs:documentation>Internal code of instituion</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="paid" use="required" type="e:Id">
			<xs:annotation>
				<xs:documentation>Party ID</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="t" use="required" type="xs:string">
			<xs:annotation>
				<xs:documentation>Type of institution. This is code of type.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="Institutions">
		<xs:annotation>
			<xs:documentation>List of institutions</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="inst" type="e:Institution" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Institution description</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Level">
		<xs:annotation>
			<xs:documentation>Description level</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Description item</xs:documentation>
				</xs:annotation>
				<xs:element name="dd" type="e:DescriptionItemDecimal" minOccurs="1" maxOccurs="1"/>
				<xs:element name="do" type="e:DescriptionItemStructObjectRef" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Description item for packet</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="dp" type="e:DescriptionItemPartyRef" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Party reference</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="dt" type="e:DescriptionItemUnitDate" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Description item date</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ds" type="e:DescriptionItemString" minOccurs="1" maxOccurs="1"/>
				<xs:element name="de" type="e:DescriptionItemEnum" minOccurs="1" maxOccurs="1"/>
				<xs:element name="di" type="e:DescriptionItemInteger" minOccurs="1" maxOccurs="1"/>
				<xs:element name="du" type="e:DescriptionItemUndefined" minOccurs="1" maxOccurs="1"/>
