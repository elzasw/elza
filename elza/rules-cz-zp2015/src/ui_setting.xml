<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<settings xmlns:ns2="fund-views" xmlns:ns3="type-groups" xmlns:ns4="allow-structure-types" 
          xmlns:ns5="fund-issues" xmlns:ns6="grid-view" xmlns:ns7="structure-type-settings"
          xmlns:ns8="parts-order" xmlns:ns9="item-types" xmlns:ns10="dao-import-level">
    <favorite-item-specs code="ZP2015_LANGUAGE">
        <!-- čeština -->
        <favorite-item>LNG_cze</favorite-item>
        <!-- němčina -->
        <favorite-item>LNG_ger</favorite-item>
        <!-- latina -->
        <favorite-item>LNG_lat</favorite-item>
    </favorite-item-specs>

    <!-- Výchozí prvky pro tabulkové zobrazení -->
    <grid-view>
        <!-- Referenční označení (pořadové číslo) -->
        <ns6:item-type code="ZP2015_UNIT_ID">
            <show-default>true</show-default>
            <width>100</width>
        </ns6:item-type>
        <!-- Jiná označení - Pořadové číslo -->
        <ns6:item-type code="ZP2015_SERIAL_NUMBER">
            <show-default>true</show-default>
            <width>50</width>
        </ns6:item-type>
        <ns6:item-type code="ZP2015_LEVEL_TYPE">
            <show-default>true</show-default>
            <width>100</width>
        </ns6:item-type>
        <ns6:item-type code="ZP2015_TITLE">
            <show-default>true</show-default>
            <width>250</width>
        </ns6:item-type>
        <ns6:item-type code="ZP2015_UNIT_DATE">
            <show-default>true</show-default>
            <width>100</width>
        </ns6:item-type>
        <ns6:item-type code="ZP2015_FOLDER_TYPE">
            <show-default>true</show-default>
            <width>100</width>
        </ns6:item-type>
        <ns6:item-type code="ZP2015_UNIT_TYPE">
            <show-default>true</show-default>
            <width>150</width>
        </ns6:item-type>
        <ns6:item-type code="ZP2015_STORAGE_ID">
            <show-default>true</show-default>
            <width>100</width>
        </ns6:item-type>
        <!-- Pořadí uložení -->
        <ns6:item-type code="ZP2015_ITEM_ORDER">
            <show-default>true</show-default>
            <width>100</width>
        </ns6:item-type>
    </grid-view>
    <structure-type-settings code="STRUCT_TYPE_ZP2015_PACKET">
      <ns7:property code="prefixSeparator"> </ns7:property>
      <ns7:property code="postfixSeparator"> </ns7:property>
    </structure-type-settings>
    <dao-import-level-settings settings-type="DAO_LEVEL_IMPORT" entity-type="FUND">
      <ns10:default>
        <scenario>Série</scenario>
        <desc-item-type>ZP2015_TITLE</desc-item-type>
      </ns10:default>
    </dao-import-level-settings>
</settings>
