      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "@typescript-eslint/no-angle-bracket-type-assertion": "off",
      "@typescript-eslint/no-unused-vars": "off",
