rule "R_REL_002 Použití typu vztahu u odkazu na jinou entitu"
when $part: Part(parent == null)
     $it: ItemType(code == "REL_ENTITY")
     $is : ItemSpec(code in ("RT_GRANDMOTHER","RT_BROTHER","RT_HECOUSIN","RT_RELATIONS","RT_GRANDFATHER","RT_GENUSMEMBER","RT_OTHERNAME","RT_ANCESTOR","RT_PLACE","RT_PLACEACT","RT_SCOPEFIELD","RT_PARTNER","RT_SHEPARTNER","RT_SISTER","RT_SHECOUSIN","RT_RESIDENCE","RT_UNCLE","RT_BROTHERINLAW","RT_SISTERINLAW","RT_AUNT","RT_FATHERINLAW","RT_MOTHERINLAW","RT_OBJECT","RT_ACTIVITIES","RT_NAMECHANGE")) from $it.specs
then
    $is.setPossible();
    $is.setRepeatable(true);
end

rule "R_CRE_005 Použití vztahů u vzniku"
when $part: Part(parent != null)
     $parent: Part(type == PartType.PT_CRE) from $part.parent
     $parentItem: Item(type == "CRE_CLASS", spec == "CRC_BEGINSCOPE") from $parent.items
     $it: ItemType(code == "REL_ENTITY")
     $is : ItemSpec(code in ("RT_DOCUMENT")) from $it.specs
then
    $is.setPossible();
    $is.setRepeatable(true);
end

