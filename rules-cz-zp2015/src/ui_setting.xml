            <accordion-right>ZP2015_FOLDER_TYPE ZP2015_STORAGE_ID</accordion-right>
            <hierarchy-level code="ZP2015_LEVEL_TYPE">
                <hierarchy-item code="ZP2015_LEVEL_ROOT">
                    <icon>fa-sitemap</icon>
                    <separator-first>||</separator-first>
                    <separator-other>|</separator-other>
                </hierarchy-item>
                <hierarchy-item code="ZP2015_LEVEL_SECTION">
                    <icon>fa-sitemap</icon>
                    <separator-first>||</separator-first>
                    <separator-other>|</separator-other>
                </hierarchy-item>
                <hierarchy-item code="ZP2015_LEVEL_SERIES">
                    <icon>ez-serie</icon>
                    <separator-first>||</separator-first>
                    <separator-other>|</separator-other>
                </hierarchy-item>
                <hierarchy-item code="ZP2015_LEVEL_FOLDER">
                    <icon>fa-folder-o</icon>
                    <separator-first>||</separator-first>
                    <separator-other>|</separator-other>
                </hierarchy-item>
                <hierarchy-item code="ZP2015_LEVEL_ITEM">
                    <icon>fa-file-text-o</icon>
                    <separator-first>||</separator-first>
                    <separator-other>|</separator-other>
                </hierarchy-item>
                <hierarchy-item code="ZP2015_LEVEL_PART">
                    <icon>ez-item-part-o</icon>
                    <separator-first>||</separator-first>
                    <separator-other>|</separator-other>
                </hierarchy-item>
            </hierarchy-level>
        </default>
